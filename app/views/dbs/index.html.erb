<dl class="db-list">
<% @dbs.each_with_index do |db, index| %>
  <dt>
    <span class="position"><%= index + 1 %></span>
    <span class="name"><%= link_to db.name, db %></span>
  </dt>
<% end %>
</dl>

<ul
  <li><%= link_to "Version #1", "#", :id => "version-1" %></li>
  <li><%= link_to "Version #2", "#", :id => "version-2" %></li>
</ul>